{
    "name": "fastsurvey-frontend",
    "version": "0.7.0",
    "main": "server.js",
    "author": {
        "name": "Moritz Makowski",
        "email": "moritz@dostuffthatmatters.dev",
        "url": "https://dostuffthatmatters.dev/"
    },
    "license": "UNLICENSED",
    "scripts": {
        "develop": "vite --host",
        "build": "yarn build:documentation && yarn build:app",
        "build:documentation": "cd ../fastsurvey-docs && yarn build && cd ../fastsurvey-frontend",
        "build:app": "export VITE_COMMIT_SHA=\"$(git rev-parse --short --verify HEAD)\" && tsc && vite build && mv dist/app/dist/app/assets dist/app/assets && rmdir dist/app/dist/app dist/app/dist",
        "serve": "vite preview",
        "analyze": "source-map-explorer 'dist/assets/*.js'",
        "cypress:integration": "cypress open",
        "cypress:component": "yarn tailwind:build && cypress open-ct",
        "tailwind:build": "npx tailwindcss -i src/styles/tailwind.css -o src/styles/tailwind.out.css"
    },
    "dependencies": {
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@cypress/react": "^5.10.1",
        "@cypress/vite-dev-server": "^2.2.0",
        "@cypress/webpack-dev-server": "^1.7.0",
        "@sentry/react": "^6.15.0",
        "@tailwindcss/typography": "^0.5.0",
        "@types/jest": "^26.0.23",
        "@types/js-cookie": "^2.2.7",
        "@types/lodash": "^4.14.170",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-router-dom": "^5.1.7",
        "@vitejs/plugin-react-refresh": "^1.3.1",
        "autoprefixer": "^10.4.1",
        "axios": "^0.21.1",
        "cypress": "8.7.0",
        "jest": "^27.0.6",
        "js-cookie": "^3.0.1",
        "lodash": "^4.17.21",
        "postcss": "^8.4.5",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "react-markdown": "^7.1.2",
        "react-redux": "^7.2.4",
        "react-router-dom": "^5.2.0",
        "redux": "^4.1.1",
        "remark-gfm": "^3.0.1",
        "remark-math": "^5.1.1",
        "tailwindcss": "^3.0.8",
        "typeface-quicksand": "^1.1.13",
        "typescript": "^4.3.2",
        "vite": "^2.3.8",
        "webpack-dev-server": "^4.3.1"
    }
}
